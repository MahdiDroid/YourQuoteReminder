@model YourQuoteReminder.ViewModels.QuoteViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Quote Of Day</h1>
<table class="table ">
    <tr>
        <th>ID</th>
        <th>UserQuote </th>
        <th>CreatedDateTime</th>
    </tr>

    @foreach (var item in Model.Quotes)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.UserQuote</td>
            <td>@item.CreatedDateTime</td>
        </tr>

    }


</table>

